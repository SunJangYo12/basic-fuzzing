<font></font>
    <span class="n">p</span> <span class="o">=</span> <span class="n">Popen</span><span class="p">([</span><span class="s">"exiv2"</span><span class="p">,</span> <span class="s">"pr"</span><span class="p">,</span> <span class="s">"-v"</span><span class="p">,</span> <span class="s">"mutated.jpg"</span><span class="p">],</span> <span class="n">stdout</span><span class="o">=</span><span class="n">PIPE</span><span class="p">,</span> <span class="n">stderr</span><span class="o">=</span><span class="n">PIPE</span><span class="p">)</span>
    <span class="p">(</span><span class="n">out</span><span class="p">,</span><span class="n">err</span><span class="p">)</span> <span class="o">=</span> <span class="n">p</span><span class="p">.</span><span class="n">communicate</span><span class="p">()</span><font></font>
<font></font>
    <span class="k">if</span> <span class="n">p</span><span class="p">.</span><span class="n">returncode</span> <span class="o">==</span> <span class="o">-</span><span class="mi">11</span><span class="p">:</span>
    	<span class="n">f</span> <span class="o">=</span> <span class="nb">open</span><span class="p">(</span><span class="s">"crashes2/crash.{}.jpg"</span><span class="p">.</span><span class="nb">format</span><span class="p">(</span><span class="nb">str</span><span class="p">(</span><span class="n">counter</span><span class="p">)),</span> <span class="s">"ab+"</span><span class="p">)</span>
    	<span class="n">f</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="n">data</span><span class="p">)</span>
    	<span class="k">print</span><span class="p">(</span><span class="s">"Segfault!"</span><span class="p">)</span><font></font>
<font></font>
    <span class="k">elif</span> <span class="n">p</span><span class="p">.</span><span class="n">returncode</span> <span class="o">==</span> <span class="o">-</span><span class="mi">8</span><span class="p">:</span>
    	<span class="n">f</span> <span class="o">=</span> <span class="nb">open</span><span class="p">(</span><span class="s">"crashes2/crash.{}.jpg"</span><span class="p">.</span><span class="nb">format</span><span class="p">(</span><span class="nb">str</span><span class="p">(</span><span class="n">counter</span><span class="p">)),</span> <span class="s">"ab+"</span><span class="p">)</span>
    	<span class="n">f</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="n">data</span><span class="p">)</span>
    	<span class="k">print</span><span class="p">(</span><span class="s">"Floating Point!"</span><span class="p">)</span>
</code></pre></div></div>

