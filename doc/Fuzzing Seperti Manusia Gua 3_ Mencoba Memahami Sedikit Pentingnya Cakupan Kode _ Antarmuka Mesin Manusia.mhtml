        <font></font>
<font></font>
        <span class="n">execvp</span><span class="p">(</span><span class="n">file</span><span class="p">,</span> <span class="n">argv</span><span class="p">);</span>
        <span class="c1">// shouldn't return, if it does, we have an error with the command</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[!] Unknown command for execvp, exiting...</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span>
    <span class="k">else</span> <span class="p">{</span>
        <span class="c1">// this is run by the parent process</span>
        <span class="k">do</span> <span class="p">{</span>
            <span class="n">pid_t</span> <span class="n">tpid</span> <span class="o">=</span> <span class="n">waitpid</span><span class="p">(</span><span class="n">child_pid</span><span class="p">,</span> <span class="o">&amp;</span><span class="n">child_status</span><span class="p">,</span> <span class="n">WUNTRACED</span> <span class="o">|</span>
             <span class="n">WCONTINUED</span><span class="p">);</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">tpid</span> <span class="o">==</span> <span class="o">-</span><span class="mi">1</span><span class="p">)</span> <span class="p">{</span>
                <span class="n">printf</span><span class="p">(</span><span class="s">"[!] Waitpid failed!</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
                <span class="n">perror</span><span class="p">(</span><span class="s">"waitpid"</span><span class="p">);</span>
            <span class="p">}</span>
            <span class="k">if</span> <span class="p">(</span><span class="n">WIFEXITED</span><span class="p">(</span><span class="n">child_status</span><span class="p">))</span> <span class="p">{</span>
                <span class="c1">//printf("WIFEXITED: Exit Status: %d\n", WEXITSTATUS(child_status));</span>
            <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="n">WIFSIGNALED</span><span class="p">(</span><span class="n">child_status</span><span class="p">))</span> <span class="p">{</span>
                <span class="n">crashes</span><span class="o">++</span><span class="p">;</span>
                <span class="kt">int</span> <span class="n">exit_status</span> <span class="o">=</span> <span class="n">WTERMSIG</span><span class="p">(</span><span class="n">child_status</span><span class="p">);</span>
                <span class="n">printf</span><span class="p">(</span><span class="s">"</span><span class="se">\r</span><span class="s">[&gt;] Crashes: %d"</span><span class="p">,</span> <span class="n">crashes</span><span class="p">);</span>
                <span class="n">fflush</span><span class="p">(</span><span class="n">stdout</span><span class="p">);</span>
                <span class="kt">char</span> <span class="n">command</span><span class="p">[</span><span class="mi">50</span><span class="p">];</span>
                <span class="n">sprintf</span><span class="p">(</span><span class="n">command</span><span class="p">,</span> <span class="s">"cp mutated.jpeg ccrashes/%d.%d"</span><span class="p">,</span> <span class="n">iteration</span><span class="p">,</span> 
                <span class="n">exit_status</span><span class="p">);</span>
                <span class="n">system</span><span class="p">(</span><span class="n">command</span><span class="p">);</span>
            <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="n">WIFSTOPPED</span><span class="p">(</span><span class="n">child_status</span><span class="p">))</span> <span class="p">{</span>
                <span class="n">printf</span><span class="p">(</span><span class="s">"WIFSTOPPED: Exit Status: %d</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">WSTOPSIG</span><span class="p">(</span><span class="n">child_status</span><span class="p">));</span>
            <span class="p">}</span> <span class="k">else</span> <span class="k">if</span> <span class="p">(</span><span class="n">WIFCONTINUED</span><span class="p">(</span><span class="n">child_status</span><span class="p">))</span> <span class="p">{</span>
                <span class="n">printf</span><span class="p">(</span><span class="s">"WIFCONTINUED: Exit Status: Continued.</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
            <span class="p">}</span>
        <span class="p">}</span> <span class="k">while</span> <span class="p">(</span><span class="o">!</span><span class="n">WIFEXITED</span><span class="p">(</span><span class="n">child_status</span><span class="p">)</span> <span class="o">&amp;&amp;</span> <span class="o">!</span><span class="n">WIFSIGNALED</span><span class="p">(</span><span class="n">child_status</span><span class="p">));</span>
    <span class="p">}</span>
<span class="p">}</span><font></font>
<font></font>
<span class="kt">int</span> <span class="nf">main</span><span class="p">(</span><span class="kt">int</span> <span class="n">argc</span><span class="p">,</span> <span class="kt">char</span><span class="o">**</span> <span class="n">argv</span><span class="p">)</span> <span class="p">{</span><font></font>
<font></font>
    <span class="k">if</span> <span class="p">(</span><span class="n">argc</span> <span class="o">&lt;</span> <span class="mi">3</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"Usage: ./cfuzz &lt;valid jpeg&gt; &lt;num of fuzz iterations&gt;</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"Usage: ./cfuzz Canon_40D.jpg 10000</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span><font></font>
<font></font>
    <span class="c1">// get our random seed</span>
    <span class="n">srand</span><span class="p">((</span><span class="kt">unsigned</span><span class="p">)</span><span class="n">time</span><span class="p">(</span><span class="nb">NULL</span><span class="p">));</span><font></font>
<font></font>
    <span class="kt">char</span><span class="o">*</span> <span class="n">fuzz_target</span> <span class="o">=</span> <span class="n">argv</span><span class="p">[</span><span class="mi">1</span><span class="p">];</span>
    <span class="k">struct</span> <span class="n">ORIGINAL_FILE</span> <span class="n">original_file</span> <span class="o">=</span> <span class="n">get_data</span><span class="p">(</span><span class="n">fuzz_target</span><span class="p">);</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] Size of file: %ld bytes.</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">original_file</span><span class="p">.</span><span class="n">length</span><span class="p">);</span>
    <span class="kt">size_t</span> <span class="n">mutations</span> <span class="o">=</span> <span class="p">(</span><span class="n">original_file</span><span class="p">.</span><span class="n">length</span> <span class="o">-</span> <span class="mi">4</span><span class="p">)</span> <span class="o">*</span> <span class="p">.</span><span class="mo">02</span><span class="p">;</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] Flipping up to %ld bytes.</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">mutations</span><span class="p">);</span><font></font>
<font></font>
    <span class="kt">int</span> <span class="n">iterations</span> <span class="o">=</span> <span class="n">atoi</span><span class="p">(</span><span class="n">argv</span><span class="p">[</span><span class="mi">2</span><span class="p">]);</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] Fuzzing for %d iterations...</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">iterations</span><span class="p">);</span>
    <span class="k">for</span> <span class="p">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">iterations</span><span class="p">;</span> <span class="n">i</span><span class="o">++</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">create_new</span><span class="p">(</span><span class="n">original_file</span><span class="p">,</span> <span class="n">mutations</span><span class="p">);</span>
        <span class="n">exif</span><span class="p">(</span><span class="n">i</span><span class="p">);</span>
    <span class="p">}</span><font></font>
    <font></font>
    <span class="n">printf</span><span class="p">(</span><span class="s">"</span><span class="se">\n</span><span class="s">[&gt;] Fuzzing completed, exiting...</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
    <span class="k">return</span> <span class="mi">0</span><span class="p">;</span>
<span class="p">}</span>
</code></pre></div></div>

<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Tidak akan menghabiskan banyak waktu untuk membahas fitur fuzzer (fitur apa?) di sini, tetapi beberapa hal penting tentang kode fuzzer:</font></font></p>
<ul>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">mengambil file sebagai input dan menyalin byte dari file ke buffer</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">ini menghitung panjang buffer dalam byte, dan kemudian mengubah 2% byte dengan menimpanya secara acak dengan byte yang sembarangan</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">fungsi yang bertanggung jawab atas mutasi, </font></font><code class="language-plaintext highlighter-rouge">create_new</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">, tidak melacak indeks byte mana yang dimutasi, jadi secara teoritis, indeks yang sama dapat dipilih untuk mutasi beberapa kali, jadi sebenarnya, fuzzer memutasi </font></font><strong><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">hingga</font></font></strong><font style="vertical-align: inherit;"><font style="vertical-align: inherit;"> 2% byte.</font></font></li>
</ul>

<h3 id="small-detour-i-apologize"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Jalan memutar kecil, saya minta maaf</font></font></h3>
<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Kita hanya memiliki satu metode mutasi di sini untuk menjaga semuanya tetap sederhana, dengan melakukannya, saya benar-benar mempelajari sesuatu yang sangat berguna yang sebelumnya tidak saya pikirkan dengan jelas. Dalam posting sebelumnya saya bertanya-tanya, dengan malu, dengan lantang dan tercetak, seberapa jauh perbedaan antara pembalikan bit acak dan penimpaan byte acak (pembalikan?). Nah, ternyata, keduanya sangat berbeda. Mari kita luangkan waktu sebentar untuk melihat perbedaannya.</font></font></p>

<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Katakanlah kita melakukan mutasi pada array byte yang disebut </font></font><code class="language-plaintext highlighter-rouge">bytes</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">. Kita melakukan mutasi indeks 5. </font></font><code class="language-plaintext highlighter-rouge">bytes[5]</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">== </font></font><code class="language-plaintext highlighter-rouge">\x41</code> <code class="language-plaintext highlighter-rouge">(65 in decimal)</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">pada file asli yang tidak dimutasi dan murni. Jika kita </font></font><em><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">hanya</font></font></em><font style="vertical-align: inherit;"><font style="vertical-align: inherit;"> melakukan bit flip, kita sangat terbatas dalam hal seberapa banyak kita dapat melakukan mutasi pada byte ini. 65 dalam </font></font><code class="language-plaintext highlighter-rouge">01000001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">bentuk biner. Mari kita lihat seberapa banyak perubahan yang terjadi akibat pembalikan satu bit secara sembarangan:</font></font></p>
<ul>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit pertama: </font></font><code class="language-plaintext highlighter-rouge">11000001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 193,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit kedua: </font></font><code class="language-plaintext highlighter-rouge">00000001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 1,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit ketiga: </font></font><code class="language-plaintext highlighter-rouge">01100001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 97,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit keempat: </font></font><code class="language-plaintext highlighter-rouge">01010001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 81,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit kelima: </font></font><code class="language-plaintext highlighter-rouge">01001001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 73,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit keenam: </font></font><code class="language-plaintext highlighter-rouge">01000101</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 69,</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit ketujuh: </font></font><code class="language-plaintext highlighter-rouge">01000011</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 67, dan</font></font></li>
  <li><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Membalik bit kedelapan: </font></font><code class="language-plaintext highlighter-rouge">010000001</code><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">= 64.</font></font></li>
</ul>

<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Seperti yang Anda lihat, kita terkunci pada kemungkinan yang sangat terbatas.</font></font></p>

<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Jadi untuk program ini, saya memilih untuk mengganti metode mutasi ini dengan metode yang hanya mengganti byte acak, bukan bit dalam byte.</font></font></p>

<h2 id="vulnerable-program"><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Program Rentan</font></font></h2>
<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Saya menulis program kartun sederhana untuk menunjukkan betapa sulitnya bagi para fuzzer yang "bodoh" untuk menemukan bug. Bayangkan sebuah aplikasi target yang memiliki beberapa pohon keputusan dalam tampilan peta pembongkaran biner. Aplikasi tersebut melakukan 2-3 pemeriksaan pada input untuk melihat apakah input tersebut memenuhi kriteria tertentu sebelum meneruskan input tersebut ke beberapa jenis fungsi yang rentan. Inilah yang saya maksud:</font></font></p>

<p><img src="https://h0mbre.github.io/assets/images/AWE/tree.PNG" alt=""></p>

<p><font style="vertical-align: inherit;"><font style="vertical-align: inherit;">Program kami melakukan hal yang persis seperti ini, mengambil byte dari file input dan memeriksa byte pada indeks 1/3 dari panjang file, 1/2 dari panjang file, dan 2/3 dari panjang file untuk melihat apakah byte pada posisi tersebut cocok dengan beberapa nilai yang dikodekan secara kaku (arbitrer). Jika semua pemeriksaan berhasil, aplikasi menyalin buffer byte ke buffer kecil yang menyebabkan segfault untuk mensimulasikan fungsi yang rentan. Berikut adalah program kami:</font></font></p>
<div class="language-c highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="cp">#include</span> <span class="cpf">&lt;stdio.h&gt;</span><span class="cp">
#include</span> <span class="cpf">&lt;stdlib.h&gt;</span><span class="cp">
#include</span> <span class="cpf">&lt;string.h&gt;</span><span class="cp">
#include</span> <span class="cpf">&lt;errno.h&gt;</span><span class="cp">
</span>
<span class="k">struct</span> <span class="n">ORIGINAL_FILE</span> <span class="p">{</span>
    <span class="kt">char</span> <span class="o">*</span> <span class="n">data</span><span class="p">;</span>
    <span class="kt">size_t</span> <span class="n">length</span><span class="p">;</span>
<span class="p">};</span><font></font>
<font></font>
<span class="k">struct</span> <span class="n">ORIGINAL_FILE</span> <span class="nf">get_bytes</span><span class="p">(</span><span class="kt">char</span><span class="o">*</span> <span class="n">fileName</span><span class="p">)</span> <span class="p">{</span><font></font>
<font></font>
    <span class="kt">FILE</span> <span class="o">*</span><span class="n">filePtr</span><span class="p">;</span>
    <span class="kt">char</span><span class="o">*</span> <span class="n">buffer</span><span class="p">;</span>
    <span class="kt">long</span> <span class="n">fileLen</span><span class="p">;</span><font></font>
<font></font>
    <span class="n">filePtr</span> <span class="o">=</span> <span class="n">fopen</span><span class="p">(</span><span class="n">fileName</span><span class="p">,</span> <span class="s">"rb"</span><span class="p">);</span>
    <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="n">filePtr</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] Unable to open %s</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">fileName</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span><font></font>
    <font></font>
    <span class="k">if</span> <span class="p">(</span><span class="n">fseek</span><span class="p">(</span><span class="n">filePtr</span><span class="p">,</span> <span class="mi">0</span><span class="p">,</span> <span class="n">SEEK_END</span><span class="p">))</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] fseek() failed, wtf?</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span><font></font>
<font></font>
    <span class="n">fileLen</span> <span class="o">=</span> <span class="n">ftell</span><span class="p">(</span><span class="n">filePtr</span><span class="p">);</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">fileLen</span> <span class="o">==</span> <span class="o">-</span><span class="mi">1</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] ftell() failed, wtf?</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span><font></font>
<font></font>
    <span class="n">errno</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span>
    <span class="n">rewind</span><span class="p">(</span><span class="n">filePtr</span><span class="p">);</span>
    <span class="k">if</span> <span class="p">(</span><span class="n">errno</span><span class="p">)</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] rewind() failed, wtf?</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span><font></font>
<font></font>
    <span class="kt">long</span> <span class="n">trueSize</span> <span class="o">=</span> <span class="n">fileLen</span> <span class="o">*</span> <span class="k">sizeof</span><span class="p">(</span><span class="kt">char</span><span class="p">);</span>
    <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] %s is %ld bytes.</span><span class="se">\n</span><span class="s">"</span><span class="p">,</span> <span class="n">fileName</span><span class="p">,</span> <span class="n">trueSize</span><span class="p">);</span>
    <span class="n">buffer</span> <span class="o">=</span> <span class="p">(</span><span class="kt">char</span> <span class="o">*</span><span class="p">)</span><span class="n">malloc</span><span class="p">(</span><span class="n">fileLen</span> <span class="o">*</span> <span class="k">sizeof</span><span class="p">(</span><span class="kt">char</span><span class="p">));</span>
    <span class="n">fread</span><span class="p">(</span><span class="n">buffer</span><span class="p">,</span> <span class="n">fileLen</span><span class="p">,</span> <span class="mi">1</span><span class="p">,</span> <span class="n">filePtr</span><span class="p">);</span>
    <span class="n">fclose</span><span class="p">(</span><span class="n">filePtr</span><span class="p">);</span><font></font>
<font></font>
    <span class="k">struct</span> <span class="n">ORIGINAL_FILE</span> <span class="n">original_file</span><span class="p">;</span>
    <span class="n">original_file</span><span class="p">.</span><span class="n">data</span> <span class="o">=</span> <span class="n">buffer</span><span class="p">;</span>
    <span class="n">original_file</span><span class="p">.</span><span class="n">length</span> <span class="o">=</span> <span class="n">trueSize</span><span class="p">;</span><font></font>
<font></font>
    <span class="k">return</span> <span class="n">original_file</span><span class="p">;</span>
<span class="p">}</span><font></font>
<font></font>
<span class="kt">void</span> <span class="nf">check_one</span><span class="p">(</span><span class="kt">char</span><span class="o">*</span> <span class="n">buffer</span><span class="p">,</span> <span class="kt">int</span> <span class="n">check</span><span class="p">)</span> <span class="p">{</span><font></font>
<font></font>
    <span class="k">if</span> <span class="p">(</span><span class="n">buffer</span><span class="p">[</span><span class="n">check</span><span class="p">]</span> <span class="o">==</span> <span class="sc">'\x6c'</span><span class="p">)</span> <span class="p">{</span>
        <span class="k">return</span><span class="p">;</span>
    <span class="p">}</span>
    <span class="k">else</span> <span class="p">{</span>
        <span class="n">printf</span><span class="p">(</span><span class="s">"[&gt;] Check 1 failed.</span><span class="se">\n</span><span class="s">"</span><span class="p">);</span>
        <span class="n">exit</span><span class="p">(</span><span class="o">-</span><span class="mi">1</span><span class="p">);</span>
    <span class="p">}</span>
<span class="p">}</span><font></font>
<font></font>
<span class="kt">void</span> <span class="nf">check_two</span><span class="p">(</span><span class="kt">char</span><span class="o">*</span> <span class="n">buffer</span><span class="p">,</span> <span class="kt">int</span> <span class="n">check</span><span class="p">)</span> <span class="p">{</span><font></font>
<font></font>
