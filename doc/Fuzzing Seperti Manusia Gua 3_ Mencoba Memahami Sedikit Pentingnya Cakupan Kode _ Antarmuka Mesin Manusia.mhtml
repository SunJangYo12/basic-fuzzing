<font></font>
<span class="kt">void</span> <span class="nf">exif</span><span class="p">(</span><span class="kt">int</span> <span class="n">iteration</span><span class="p">)</span> <span class="p">{</span><font></font>
    <font></font>
    <span class="c1">//fileptr = popen("exiv2 pr -v mutated.jpeg &gt;/dev/null 2&gt;&amp;1", "r");</span>
    <span class="kt">char</span><span class="o">*</span> <span class="n">file</span> <span class="o">=</span> <span class="s">"vuln"</span><span class="p">;</span>
    <span class="kt">char</span><span class="o">*</span> <span class="n">argv</span><span class="p">[</span><span class="mi">3</span><span class="p">];</span>
    <span class="n">argv</span><span class="p">[</span><span class="mi">0</span><span class="p">]</span> <span class="o">=</span> <span class="s">"vuln"</span><span class="p">;</span>
    <span class="n">argv</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span> <span class="o">=</span> <span class="s">"mutated.jpeg"</span><span class="p">;</span>
    <span class="n">argv</span><span class="p">[</span><span class="mi">2</span><span class="p">]</span> <span class="o">=</span> <span class="nb">NULL</span><span class="p">;</span>
    <span class="n">pid_t</span> <span class="n">child_pid</span><span class="p">;</span>
