<font></font>
<span class="kt">void</span> <span class="nf">create_new</span><span class="p">(</span><span class="k">struct</span> <span class="n">ORIGINAL_FILE</span> <span class="n">original_file</span><span class="p">,</span> <span class="kt">size_t</span> <span class="n">mutations</span><span class="p">)</span> <span class="p">{</span><font></font>
